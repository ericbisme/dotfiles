Host *
    ForwardAgent yes
    ServerAliveInterval 60

Host *.cu.edu
    IdentityFile ~/.ssh/cu

Host *.github.com
    IdentityFile ~/.ssh/github.key

Host *.gitlab.com
    IdentityFile ~/.ssh/gitlab.key

Host *.bitbucket.com
    IdentityFile ~/.ssh/bitbucket.key

host *.ericbisme.net
    IdentityFile ~/.ssh/ericbismenet

host *.amazonaws.com
    IdentityFile ~/.ssh/key1.pem

host *.ericbisme.io
    IdentityFile ~/.ssh/key1.pem

Host *.dev.lanyapps.com
  IdentityFile ~/.ssh/dev-pum-instances.lanyapps.pem
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  User ec2-user
  LogLevel QUIET

Host cs92modo.rojoserve.com
  IdentityFile ~/.ssh/rojoserve
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  User ebolinger
  LogLevel QUIET
